{
  "name": "basic-language-extension",
  "displayName": "CPCReady Basic Language (AMSTRAD)",
  "description": "Programacion Amstrad Basic and ugBasic",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "CPCReady",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/CPCReady/basic-language-extension"
  },
  "keywords": [
    "AMSTRAD",
    "Basic",
    "ugBasic"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cpcready-language",
        "aliases": [
          "amstrad-basic-cpcready",
          "cpcready-amstrad-basic",
          "cpcready-language"
        ],
        "extensions": [
          ".bas",
          ".ugbasic",
          ".ugb",
          ".BAS",
          ".UGB",
          "UGBASIC"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cpcready-language",
        "scopeName": "source.cpcready-language",
        "path": "./syntaxes/cpcready-language.tmLanguage.json"
      }
    ]
  }
}
